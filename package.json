{
  "name": "balancefit",
  "version": "0.0.1",
  "description": "Awesome node project developed with dike.",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "NODE_ENV=production && node --unhandled-rejections=strict --experimental-specifier-resolution=node dist/www.js",
    "start:dev": "NODE_ENV=development && node --unhandled-rejections=strict --experimental-specifier-resolution=node dist/www.js",
    "local:dev": "nodemon --watch 'src/**' --ext ts,json --exec 'npm run build && npm run start' --delay 2",
    "typeorm": "typeorm-ts-node-esm",
    "clean": "rm -rf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dev-info": "[info]_moduleAliases apply running environment",
  "_moduleAliases": {
    "#": "dist",
    "#config": "../config",
    "#types": "./dist/@types"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.258.0",
    "@aws-sdk/lib-storage": "^3.245.0",
    "aws-sdk": "^2.1289.0",
    "axios": "^1.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "formidable": "^3.2.5",
    "get-video-duration": "^4.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.20",
    "npm": "^9.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.0",
    "prettier": "^2.8.2",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.31.2",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/formidable": "^2.0.5",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.21",
    "@types/node-cron": "^3.0.6",
    "@types/sharp": "^0.31.0",
    "cross-env": "^7.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "ts-node": "10.7.0",
    "tsc-alias": "^1.7.0",
    "typescript": "4.5.2"
  }
}
